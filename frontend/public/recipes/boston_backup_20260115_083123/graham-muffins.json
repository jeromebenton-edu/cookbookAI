  },
  "ingredients": [
    "1 cup graham flour",
    "1 cup sifted flour",
    "2 tbsp brown sugar",
    "2 tsp baking powder",
    "1/2 tsp salt",
    "1 cup milk",
    "1 egg, beaten",
    "2 tbsp molasses",
    "2 tbsp melted butter"
  ],
  "instructions": [
    "Sift dry ingredients together in a mixing bowl.",
    "Whisk milk, egg, molasses, and melted butter until smooth.",
    "Combine wet and dry mixtures gently; do not overmix.",
    "Fill greased muffin cups two-thirds full and bake at 400F for 18 minutes."
  ],
  "notes": [
    "Serve warm with whipped butter.",
    "Replace molasses with honey for a lighter flavor."
  ],
